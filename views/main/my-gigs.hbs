<div class="contaner">
    <h1 class="text-center">My Gigs</h1>
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <div class="overview">
                <div class="add-gig-card">
                    <p class="text-center" style="margin-top: 40px">
                        <a href="/add-new-gig" class="btn btn-success btn-lg">Add new Gig</a>
                </div>
            </div>
            <div class="overview">
                <div class="list-orders-card">
                    <div class="table-margin">
                        <table class="table">
                            <thead>
                            <tr>
                                <th>Gig</th>
                                <th>Title</th>
                            </tr>
                            </thead>
                            <tbody>
                            {{#each gigs}}
                            <tr>
                                <td><img src="{{picture}}" class="order-image-my-gig"></td>
                                <td>{{title}}</td>
                            </tr>
                            {{/each}}
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>